<!--pages/reviews/reviews.wxml-->
<view
  wx:for='{{reviewList}}'
  wx:key='{{item.key}}'
  class='review'
>
  <view class='user'>
    <image
      class='user-avatar'
      src='{{item.avatar}}'
    />
    <view class='user-name'>
      {{ item.username }}的影评
    </view>
  </view>
  <view class='content'>{{item.content}}</view>
</view>
<button>
    回到首页
</button>
